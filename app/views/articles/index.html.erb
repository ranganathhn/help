<p id="notice"><%= notice %></p>

<h1>News Data</h1>

<table>
  <thead>
    <tr>
      <th>Headlines</th>
      <th>Category</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @articles.each do |news_datum| %>
      <tr>
        <td><%= news_datum.title %></td>
        <td><%= news_datum.category.try(:name) %></td>
        </tr>
    <% end %>
  </tbody>
</table>

<br>

